package Test_01;

public class Multiplication_Tables {

	public static void main(String[] args) {
		
		int n;
		n=2;
		for(int i=1;i<=n;i++)
			
		{
			System.out.println("\n"+"Table for"+""+i+"\n");
			for(int j=1;j<=10;j++)
			{
				System.out.println(i+"x"+j+"="+i*j);
			}System.out.print("\t");
		}

	}

}
